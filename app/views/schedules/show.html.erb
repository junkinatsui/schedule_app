<div>
  <ul>
    <li><%= link_to "←一覧に戻る", :schedules %></li>
  </ul>
</div>

<table>
  <tr>
    <td><%= @schedule.title %></td>
  </tr>
  <tr>
    <th>開始日</th>
    <td><%= @schedule.start_day %></td>
  </tr>
  <tr>
    <th>終了日</th>
    <td><%= @schedule.end_day %></td>
  </tr>
    <th>終日</th>
    <td><%= @schedule.all_day ? '◯': '' %></td>
  </tr>
  <tr>
    <th>スケジュールメモ</th>
    <td><%= @schedule.memo %></td>
  </tr>
</table>
